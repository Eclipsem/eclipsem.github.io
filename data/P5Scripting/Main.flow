void OpenMM() {
  int MainMenuOptions = SEL_GENERIC_NOT_HELP(-1 , Main);
  
  switch(MainMenuOptions) {
	  case 0:
	     PLOF();
	     break;
	  case 1:
	     WROF();
	     break;
	  case 2:
		 SUB_Call_GroupChat();
	     break;
	  case 3:
	    MISCOF();
	     break;
  }
}

